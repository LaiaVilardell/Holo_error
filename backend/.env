# Rename this file to .env and fill in the values.

# This is the connection string for your database.
# For PostgreSQL: DATABASE_URL="postgresql://user:password@host:port/database"
# For SQLite (file-based): DATABASE_URL="sqlite:///./holo.db"
DATABASE_URL="sqlite:///./holo.db"

# This is a secret key for signing JWTs.
# You can generate a good one using: openssl rand -hex 32
SECRET_KEY="your-super-secret-key-that-is-long-and-random"
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=30
